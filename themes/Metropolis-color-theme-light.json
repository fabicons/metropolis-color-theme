    // SEMANTIC TOKENS
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "variable": "#A6880A",
        "variable.readonly": "#A6880A",
        "property": "#D06818",
        "function": "#E03428",
        "method": "#E03428",
        "class": {
            "foreground": "#2D8F15",
            "fontStyle": ""
        }
    },
    // TEXTMATE SCOPED
    "tokenColors": [
        {
            "name": "Comment",
            "scope": ["comment", "string.comment"],
            "settings": {
                "foreground": "#8590A0"
            }
        },
        {
            "name": "String",
            "scope": ["string", "string.template"],
            "settings": {
                "foreground": "#3D4752",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Number, Boolean, Undefined",
            "scope": ["constant.numeric", "constant.language.boolean", "constant.language.undefined", "constant.language.null"],
            "settings": {
                "foreground": "#3D4752"
            }
        },
        {
            "name": "Object Key",
            "scope": ["string.unquoted", "constant.other.object.key"],
            "settings": {
                "foreground": "#D06818"
            }
        },
        {
            "name": "Number, Boolean, Undefined",
            "scope": ["keyword.control.flow"],
            "settings": {
                "foreground": "#0E9882",
                "fontStyle": "bold italic"
            }
        },
